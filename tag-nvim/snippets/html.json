{
  "hr": {
    "prefix": "hr",
    "body": [
      "<hr>",
      "",
      "$0"
    ]
  },
  "script": {
    "prefix": "script",
    "body": [
      "<script src=\"$0\"></script>"
    ]
  },
  "div": {
    "prefix": "div",
    "body": [
      "<div id=\"$1\" class=\"$2\">",
      "  $0",
      "</div>"
    ]
  },
  "unordered list": {
    "prefix": "ul",
    "body": [
      "<ul ${1:class=\"$2\"}>$0</ul>"
    ]
  },
  "list item": {
    "prefix": "li",
    "body": [
      "<li ${1:class=\"$2\"}>$0</li>"
    ]
  }
}
